   "source": [
    "# creamos muestras cauchy con la funci√≥n que esta en numpy gamma = 1,2,5 , media = 0\n",
    "s=[]\n",
    "n = 3\n",
    "colors = ['blue', 'orange', 'green']\n",
    "gamma = [1,2,5]\n",
    "for i in range(n):\n",
    "    s.append(np.random.standard_cauchy(10**6)*gamma[i])\n",
    "    s[i] = s[i][(s[i]>-25) & (s[i]<25)]  # truncate distribution so it plots well\n",
    "    plt.hist(s[i],1000,density=True, color=colors[i])\n",
    "    plt.xlabel('valores aleatorios')\n",
    "    plt.ylabel('probabilidad')\n",
    "    plt.title('histograma funci√≥n nunmpy')\n",
    "    plt.legend(['$\\gamma_1= Estandar %d$'%gamma[0],'$\\gamma_2=%d$'%gamma[1],'$\\gamma_3=%d$'%gamma[2]])\n",
    "plt.show()"
   ]
