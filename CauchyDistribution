    "# creamos muestras cauchy con la funci√≥n que esta en numpy gamma = 1,2,5 , media = 0\n",
    "s=[]\n",
    "n = 3\n",
    "colors = ['blue', 'orange', 'green']\n",
    "theta = [2,4,6]\n",
    "N = 10**6 # N√∫mero de muestras\n",
    "for i in range(n):\n",
    "    s.append(np.random.exponential(theta[i],N))\n",
    "    plt.hist(s[i],100,density=True, color=colors[i])\n",
