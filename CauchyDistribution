   "source": [
    "def D_cauchy(N,x0, g):\n",
    "    return 1/(np.pi*g*(1+((N-x0)/g)**2))\n",
    "\n",
    "gamma_v = [1,2,5]\n",
    "x0 = 0\n",
    "N =  np.arange(-15,15,.01) \n",
    "d = list(map(lambda g:D_cauchy(N,x0,g),gamma_v))\n",
    "d = np.asmatrix(d)\n",
    "\n",
    "plt.plot(N,d.T)\n",
    "plt.xlabel('valores aleatorios')\n",
    "plt.ylabel('probabilidad')\n",
    "plt.title('histograma funci√≥n de num aleatorios')\n",
    "plt.legend(['$\\gamma_1= Estandar 1%d$'%gamma[0],'$\\gamma_2=%d$'%gamma[1],'$\\gamma_3=%d$'%gamma[2]])\n",
    "plt.show()"
   ]
