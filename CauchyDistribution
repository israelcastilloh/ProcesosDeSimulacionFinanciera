   "source": [
    "# Creamos la funci√≥n que crea muestras distribuidas exponencialmente\n",
    "def D_exponential(theta,N):\n",
    "    return -np.log(np.random.random(N))*theta\n",
    "\n",
    "theta_v = [3,4,6]\n",
    "N =  10**6\n",
    "d = []\n",
    "colors = ['blue', 'orange', 'green']\n",
    "n = 3\n",
    "for i in range (n):\n",
    "    d.append(list(map(lambda t:D_exponential(t,N),theta_v)))\n",
    "    plt.hist(d[i], 100, density=True)\n",
    "    plt.xlabel('valores aleatorios')\n",
    "    plt.ylabel('probabilidad')\n",
    "    plt.title('histograma funci√≥n nunmpy')"
   ]
  },
